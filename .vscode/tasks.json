{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
				"-lreadline",
				"-lhistory",
				"-L/usr/local/lib",
				"error/error.c",
				"builtins/echo.c",
				"builtins/exit.c",
				"builtins/pwd.c",
				"builtins/env.c",
				"builtins/export.c",
				"builtins/export_utils.c",
				"builtins/unset.c",
				"builtins/unset_utils.c",
				"builtins/cd.c",
				"builtins/cd_utils.c",
				"utils/utils.c",
				"utils/parse_envp.c",
				"utils/minishell_loop.c",
				"utils/execute.c",
				"utils/execute_utils1.c",
				"utils/execute_utils2.c",
				"utils/execute_heredoc.c",
				"utils/execute_heredoc_utils1.c",
				"parser/parser.c",
				"lexer/token_reader.c",
				"./libft/libft.a",
            ],
            "options": {
				"cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}